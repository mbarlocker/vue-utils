{
  "name": "@mbarlocker/vue-utils",
  "type": "commonjs",
  "version": "0.0.1",
  "author": "Matthew Barlocker",
  "description": "Common library of random vue utilities that I couldn't find elsewhere on the internet.",
  "license": "MIT",
  "packageManager": "yarn@4.3.1",
  "homepage": "https://github.com/mbarlocker/vue-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbarlocker/vue-utils.git"
  },
  "files": [
    "dist",
    "package.json",
    "yarn.lock",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    }
  },
  "scripts": {
    "build": "yarn compile",
    "clean": "rm -rf dist coverage",
    "compile": "yarn compile:cjs && yarn compile:esm",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "compile:esm": "tsc -p tsconfig.esm.json && ( echo '{\"type\": \"module\"}' > dist/esm/package.json )",
    "test": "env TZ=UTC vitest src --coverage --run"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.0.2",
    "@vitest/coverage-v8": "^2.0.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "lodash": "^4.17.21",
    "tslib": "^2.6.3",
    "vue": "^3.4.34",
    "vue-router": "^4.4.3"
  },
  "keywords": [
    "vue"
  ]
}
